  "scripts": {
    "build:tailwind": "postcss src/css/tailwind.css -o src/css/tailwind.generated.css",
    "watch:tailwind": "postcss -w src/css/tailwind.css -o src/css/tailwind.generated.css",
    "start": "run-p watch:tailwind start:react",
    "start:react": "react-scripts start",
    "prebuild": "npm run build:tailwind",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "aws s3 rm s3://corpsmap-appz/cumulus --recursive && aws s3 cp ./build/ s3://corpsmap-appz/cumulus --recursive"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.5",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.9.6"
